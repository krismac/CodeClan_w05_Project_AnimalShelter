<h2>Create a new Human Profile</h2>

<form method="POST" action="/paws/humans">

  <label for="first-name-field">First Name: </label>
  <input type="text" name="first_name" id="first-name-field"/>
  <br>
  <label for="last-name-field">Last Name: </label>
  <input type="text" name="last_name" id="last-name-field"/>
  <br>
    <input type="radio" name="adopter" id="adopter3" value="t" /><label for="adopter1"> Interested in adopting</label>
    <input type="radio" name="adopter" id="adopter3" value="f" /><label for="adopt2"> Interested in adopting</label>
  <br>
  <br>
    <input type="radio" name="walker" id="tr1" value="t" /><label for="tr1"> Interested in Walking</label>
    <input type="radio" name="walker" id="tr2" value="f" /><label for="tr2"> Not Interested in Walking</label>
  <br>
  <label for="human-photo-file-path-field">Photo file path (e.g. file/name.jpg) </label>
  <input type="text" name="human_photo_file_path" value="/default.jpg" id="human_photo_file_path"/>
  <br>
    <input type="submit" name="human-field" value="Submit your new record">

  </form>
